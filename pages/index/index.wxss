.home Swiper {
  height: 340rpx;
}
.home .nav {
  display: flex;
  padding: 10px;
}
.home .nav .nav_img {
  flex: 1;
}
.home .nav .nav_img image {
  width: 100%;
}
.home .index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.home .index_floor .floor_group .floor_title image {
  width: 100%;
}
.home .index_floor .floor_group .floor_list {
  overflow: hidden;
}
.home .index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  box-sizing: border-box;
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}
.home .index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232 *386 */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.home .index_floor .floor_group .floor_list navigator:nth-child(2),
.home .index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.home .index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
